{
    "info": {
        "_postman_id": "admin-restaurant-management",
        "name": "Admin Restaurant Management",
        "description": "APIs for managing restaurants and viewing their stats.",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Get Dashboard Stats",
            "request": {
                "method": "GET",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{adminToken}}",
                        "type": "text"
                    }
                ],
                "url": {
                    "raw": "{{baseUrl}}/admin/dashboard/stats/{{restaurantId}}",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "admin",
                        "dashboard",
                        "stats",
                        "{{restaurantId}}"
                    ]
                }
            },
            "response": []
        },
