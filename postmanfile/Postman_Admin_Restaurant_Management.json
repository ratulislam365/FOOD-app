        {
            "name": "Get Provider Reviews",
            "request": {
                "method": "GET",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{adminToken}}",
                        "type": "text"
                    }
                ],
                "url": {
                    "raw": "{{baseUrl}}/admin/providers/{{restaurantId}}/reviews?rating=5",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "admin",
                        "providers",
                        "{{restaurantId}}",
                        "reviews"
                    ],
                    "query": [
                        {
                            "key": "rating",
                            "value": "5",
                            "description": "Optional: Filter by star rating (1-5)"
                        },
                        {
                            "key": "page",
                            "value": "1",
                            "description": "Optional: Page number"
                        },
                        {
                            "key": "limit",
                            "value": "20",
                            "description": "Optional: Results per page"
                        }
                    ]
                }
            },
            "response": []
        }
    ],
    "variable": [
        {
            "key": "baseUrl",
            "value": "http://localhost:5000/api/v1"
        },
        {
            "key": "adminToken",
            "value": "ADMIN_JWT_TOKEN"
        },
        {
            "key": "restaurantId",
            "value": "RESTAURANT_PROVIDER_ID"
        }
    ]
